{%- style -%}
  .custom-design-radio input[type="radio"]:checked + div {
    border: 1px solid #1E54A5;
  }
  .custom-design-radio input[type="radio"]:checked + div span {
    color: #333;
  }
{%- endstyle -%}
<div class="page-width">
  {% form 'contact' %}
    {{ form.errors | default_errors }}
    <div class="max-w-[736px] w-full m-auto">
      <div class="flex justify-between gap-[40px] max-sm:flex-wrap max-sm:gap-[24px]">
          <!-- <label for="ContactFormFirstName">First Name</label> -->
        <input
          class="w-full border-b [border-bottom-style:solid] [border-bottom-color:rgb(211,_211,_211)] px-[6px] py-[12px] bg-transparent focus-visible:shadow-none outline-none max-sm:max-w-[100%]"
          type="text"
          id="ContactFormFirstName"
          name="contact[first_name]"
          placeholder="First Name *"
          required>

          <!-- <label for="ContactFormLastName">Last Name</label> -->
        <input
          class="w-full border-b [border-bottom-style:solid] [border-bottom-color:rgb(211,_211,_211)] px-[6px] py-[12px] bg-transparent focus-visible:shadow-none outline-none max-sm:max-w-[100%]"
          type="text"
          id="ContactFormLastName"
          name="contact[last_name]"
          placeholder="Last Name *"
          required>
      </div>
      <div class="input-fields gap-[40px] flex justify-between max-sm:flex-wrap max-sm:gap-[24px] max-sm:mt-[24px]">
          <!-- <label for="ContactFormEmail">Email Address</label> -->
        <input
          class=" w-full border-b [border-bottom-style:solid] [border-bottom-color:rgb(211,_211,_211)] px-[6px] py-[12px] bg-transparent focus-visible:shadow-none outline-none max-sm:max-w-[100%]"
          type="email"
          id="ContactFormEmail"
          name="contact[email]"
          placeholder="Email *"
          required>

          <!-- <label for="ContactFormEmail">Email Address</label> -->
        <input
          class="w-full border-b [border-bottom-style:solid] [border-bottom-color:rgb(211,_211,_211)] px-[6px] py-[12px] bg-transparent focus-visible:shadow-none outline-none max-sm:max-w-[100%]"
          type="tel"
          id="ContactFormTel"
          name="contact[tel]"
          placeholder="Phone *"
          required>
      </div>

    </div>

    {% comment %} Jewerly Type {% endcomment %}
    <div class="custom-design-radio jewelry-type mt-[64px]">
      <label for="jewelry-type" class="block text-center leading-[48px] font-light text-[24px] mb-[24px] ">JEWELRY TYPE</label>
      <div class="flex justify-center gap-[32px] flex-wrap max-sm:gap-[10px] max-sm:justify-start">
